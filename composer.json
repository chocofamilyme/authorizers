{
  "name": "chocofamilyme/authorizers",
  "type": "library",
  "description": "Library authorize user requests",
  "keywords": [
    "rbac",
    "check-permissions",
    "authorize"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.3"
  },
  "require-dev": {
    "vimeo/psalm": "^4.6.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Chocofamilyme\\Authorizers\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "check-style": "./vendor/bin/php-cs-fixer fix --dry-run --diff",
    "fix-style": "./vendor/bin/php-cs-fixer fix",
    "psalm": "./vendor/bin/psalm"
  }
}
